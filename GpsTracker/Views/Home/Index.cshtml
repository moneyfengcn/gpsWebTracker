@{
    Layout = null;
}
@using Microsoft.Extensions.Configuration;
@inject IConfiguration config

@{
    var CompanyName = @config["CompanyName"];
}


<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <META name="keywords" content="GPS,追踪器,监控平台,车辆,免费,JT/T808">
    <meta name="description" content="你喜欢的免费在线GPS监控平台" />
    <meta name="baidu-site-verification" content="code-8B9M19J7Az" />
    <meta name="author" content='@config["email"]' />
    <title>@CompanyName</title>

    <link rel="icon" type="image/x-icon" href="~/Home/assets/img/favicon.ico" />



    <script src="~/Home/js/all.js" crossorigin="anonymous"></script>

    <link href="~/Home/css/googlecss1.css" rel="stylesheet" />
    <link href="~/Home/css/googlecss2.css" rel="stylesheet" />

    <!-- Third party plugin CSS-->
    @*<link href="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.min.css" rel="stylesheet" />*@

    <link href="~/Home/css/magnific-popup.min.css" rel="stylesheet" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/Home/css/styles.css" rel="stylesheet" />
</head>
<body id="page-top">
    <!-- Navigation-->
    <nav class="navbar navbar-expand-lg navbar-light fixed-top py-3" id="mainNav">
        <div class="container">
            <a class="navbar-brand js-scroll-trigger" href="#page-top">@CompanyName</a>
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto my-2 my-lg-0">
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="#about">如何接入</a></li>
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="#services">服务</a></li>
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="#portfolio">产品目录</a></li>
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="#contact">联系我们</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- Masthead-->
    <header class="masthead">
        <div class="container h-100">
            <div class="row h-100 align-items-center justify-content-center text-center">
                <div class="col-lg-10 align-self-end">
                    <h1 class="text-uppercase text-white font-weight-bold">你喜欢的免费在线GPS监控平台</h1>
                    <hr class="divider my-4" />
                </div>
                <div class="col-lg-8 align-self-baseline">
                    <p class="text-white-75 font-weight-light mb-5">
                        我们可以帮助您建立更好的在线车辆追踪监控，使用我们的<B><font color='red'>@config["ProductName"]</font></B>!
                        只需注册一个账号并开始把GPS设备挂靠上来!
                    </p>
                    <a class="btn btn-primary btn-xl js-scroll-trigger" asp-controller="GPS" asp-action="RealMap">开始使用</a>
                </div>
            </div>
        </div>
    </header>
    <!-- About-->
    <section class="page-section bg-primary" id="about">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8 text-center">

                    <h2 class="text-white mt-0">我们有你需要的东西!</h2>
                    <hr class="divider light my-4" />
                    <p class="text-white-50 mb-4">
                        @CompanyName
                        有你需要的一切，让你的GPS设备运行和监控在任何时间!选择一个我们的平台，免费使用，简洁方便的主题!或者提供厂商的设备通信协议文档，我们为你定制接入！
                    </p>
                    <a class="btn btn-light btn-xl js-scroll-trigger" href="~/download/download.zip" download="设备接入说明.zip">下载设备接入文档</a>

                </div>
            </div>
        </div>
    </section>
    <!-- Services-->
    <section class="page-section" id="services">
        <div class="container">
            <h2 class="text-center mt-0">我们的服务!</h2>
            <hr class="divider my-4" />
            <div class="row">
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="mt-5">
                        <i class="fas fa-4x fa-gem text-primary mb-4"></i>
                        <h3 class="h4 mb-2">JT/T 部标设备</h3>
                        <p class="text-muted mb-0">我们支持标准的JT/T808交通部标准协议.通过TCP方式连接</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="mt-5">
                        <i class="fas fa-4x fa-laptop-code text-primary mb-4"></i>
                        <h3 class="h4 mb-2">标准设备</h3>
                        <p class="text-muted mb-0">我们支持2929标准协议，TCP和UDP两种方式</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="mt-5">
                        <i class="fas fa-4x fa-globe text-primary mb-4"></i>
                        <h3 class="h4 mb-2">超长待机设备</h3>
                        <p class="text-muted mb-0">我们支持2929协议超长待机设备并结合LBS定位，TCP和UDP两种方式</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 text-center">
                    <div class="mt-5">
                        <i class="fas fa-4x fa-heart text-primary mb-4"></i>
                        <h3 class="h4 mb-2">更多类型支持</h3>
                        <p class="text-muted mb-0">我们还可以接入更多类型的设备，欢迎与我们联系！</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Portfolio-->
    <div id="portfolio">
        <div class="container-fluid p-0">
            <div class="row no-gutters">
                <div class="col-lg-4 col-sm-6">
                    <a class="portfolio-box" href="~/Home/assets/img/portfolio/fullsize/1.jpg">
                        <img class="img-fluid" src="~/Home/assets/img/portfolio/thumbnails/1.jpg" alt="" />
                        <div class="portfolio-box-caption">
                            <div class="project-category text-white-50">Category</div>
                            <div class="project-name">Project Name</div>
                        </div>
                    </a>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <a class="portfolio-box" href="~/Home/assets/img/portfolio/fullsize/2.jpg">
                        <img class="img-fluid" src="~/Home/assets/img/portfolio/thumbnails/2.jpg" alt="" />
                        <div class="portfolio-box-caption">
                            <div class="project-category text-white-50">Category</div>
                            <div class="project-name">Project Name</div>
                        </div>
                    </a>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <a class="portfolio-box" href="~/Home/assets/img/portfolio/fullsize/3.jpg">
                        <img class="img-fluid" src="~/Home/assets/img/portfolio/thumbnails/3.jpg" alt="" />
                        <div class="portfolio-box-caption">
                            <div class="project-category text-white-50">Category</div>
                            <div class="project-name">Project Name</div>
                        </div>
                    </a>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <a class="portfolio-box" href="~/Home/assets/img/portfolio/fullsize/4.jpg">
                        <img class="img-fluid" src="~/Home/assets/img/portfolio/thumbnails/4.jpg" alt="" />
                        <div class="portfolio-box-caption">
                            <div class="project-category text-white-50">Category</div>
                            <div class="project-name">Project Name</div>
                        </div>
                    </a>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <a class="portfolio-box" href="~/Home/assets/img/portfolio/fullsize/5.jpg">
                        <img class="img-fluid" src="~/Home/assets/img/portfolio/thumbnails/5.jpg" alt="" />
                        <div class="portfolio-box-caption">
                            <div class="project-category text-white-50">Category</div>
                            <div class="project-name">Project Name</div>
                        </div>
                    </a>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <a class="portfolio-box" href="~/Home/assets/img/portfolio/fullsize/6.jpg">
                        <img class="img-fluid" src="~/Home/assets/img/portfolio/thumbnails/6.jpg" alt="" />
                        <div class="portfolio-box-caption p-3">
                            <div class="project-category text-white-50">Category</div>
                            <div class="project-name">Project Name</div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <!-- Call to action-->
    <section class="page-section bg-dark text-white">
        <div class="container text-center">
            <h2 class="mb-4">现在就开始免费使用吧!</h2>
            <a class="btn btn-light btn-xl" asp-controller="GPS" asp-action="RealMap">现在就进入平台!</a>
        </div>
    </section>
    <!-- Contact-->
    <section class="page-section" id="contact">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8 text-center">
                    <h2 class="mt-0">让我们联系和接触!</h2>
                    <hr class="divider my-4" />
                    <p class="text-muted mb-5">准备好和我们一起开始你的下一个项目了吗?加我们QQ或者发电子邮件，我们会尽快给你回复!</p>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 ml-auto text-center mb-5 mb-lg-0">
                    <i class="fas fa-phone fa-3x mb-3 text-muted"></i>
                    <div>@config["telephone"]</div>
                </div>
                <div class="col-lg-4 mr-auto text-center">
                    <i class="fas fa-envelope fa-3x mb-3 text-muted"></i>
                    <!-- Make sure to change the email address in BOTH the anchor text and the link target below!-->
                    <a class="d-block" href="@config["email"]">@config["email"]</a>
                </div>
            </div>
        </div>
    </section>
    <!-- Footer-->
    <footer class="bg-light py-5">
        <div class="container"><div class="small text-center text-muted">Copyright © 2020 - @DateTime.Now.Year @CompanyName</div></div>
    </footer>
    <!-- Bootstrap core JS-->
    @*<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>*@
    <script src="~/Home/js/jquery-3.5.1.slim.min.js"></script>

    @*<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"></script>*@
    <script src="~/Home/js/bootstrap.bundle.min.js"></script>


    <!-- Third party plugin JS-->
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>*@
    <script src="~/Home/js/jquery.easing.min.js"></script>


    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/jquery.magnific-popup.min.js"></script>*@
    <link href="~/Home/js/magnific-popup.min.css" rel="stylesheet" />

    <!-- Core theme JS-->
    <script src="~/Home/js/scripts.js"></script>

  
</body>


</html>
 
